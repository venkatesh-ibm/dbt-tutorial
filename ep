aws cloudwatch get-metric-statistics \
    --namespace AWS/PrivateLinkEndpoints \
    --metric-name BytesProcessed \
    --dimensions Name=VpcEndpointId,Value=vpce-xxxxxxxxxxxxxxxxx \
    --start-time 2024-06-28T00:00:00Z \
    --end-time 2024-08-12T00:00:00Z \
    --period 86400 \
    --statistics Sum \
    --region eu-west-1
